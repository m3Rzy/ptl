CREATE DATABASE myliss;
use myliss;

CREATE TABLE IF NOT EXISTS `pupil` {
	`ID` INT NOT NULL AUTO_INCREMENT,
	PRIMARY KEY (`ID`),
	`name` VARCHAR(80) CHARACTER SET 'utf8mb4' NOT NULL,
	`sername` VARCHAR(255) CHARACTER SET 'utf8mb4' NOT NULL,
	`phone` VARCHAR(10) CHARACTER SET 'utf8mb4' NOT NULL,
	`email` VARCHAR(255) CHARACTER SET 'utf8mb4' NOT NULL,
	`cost` INT NULL,
	`grade` INT NULL 
};